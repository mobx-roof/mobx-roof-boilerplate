{
  "parser": "babel-eslint",
  "extends": "eslint-config-airbnb",
  "rules": {
    "func-names": [0],
    "comma-dangle": [0],
    "strict": [0],
    "no-undef": [0],
    "id-length": [0],
    "no-multi-spaces": [0],
    "space-before-function-paren": [0],
    "comma-spacing": [0],
    "new-cap": [0],
    "vars-on-top": [0],
    "no-unused-vars": [0],
    "space-before-blocks":[0],
    "indent":[0],
    "space-after-keywords":[0],
    "spaced-comment":[0],
    "dot-notation":[0],
    "no-multiple-empty-lines":[0],

    "no-trailing-spaces":[0],
    "semi-spacing":[0],
    "no-loop-func":[0],
    "one-var":[0],
    "eol-last":[0],
    "no-new-func":[0],
    "max-len": [0],

    "react/prop-types":[0],
    "react/wrap-multilines": [0],
    "react/jsx-indent-props": [0],
    "react/jsx-closing-bracket-location": [0],
    "react/no-multi-comp": [0],
    "no-console": [0],

    "object-curly-spacing": [0],
    "space-in-parens": [2, "never"],

    "prefer-const": [0],
    "no-alert": [0],
    "react/jsx-no-bind": [0]
  },
  "ecmaFeatures": {
    "experimentalObjectRestSpread": true
  }
}
